{
    "name": "andreo/eventsauce-aggregate",
    "description": "Extended aggregate components for EventSauce.",
    "keywords": [
        "eventsauce",
        "components",
        "aggregate"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrzej Pakula",
            "email": "andrze.pakula@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "eventsauce/test-utilities": "dev-main",
        "eventsauce/uuid-encoding": "^0.2.0",
        "phpstan/phpstan": "^1.3",
        "phpunit/phpunit": "^9.4",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Andreo\\EventSauce\\Aggregate\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
